# Ditronics YouTube Downloader - Environment Variables
# Copy this file to .env and modify as needed

# Django Settings
DJANGO_SECRET_KEY=your-super-secret-key-change-this-in-production
DJANGO_DEBUG=False

# Allowed hosts (comma-separated if multiple)
DJANGO_ALLOWED_HOSTS=yourdomain.com,localhost,127.0.0.1

# CSRF Trusted Origins (include full URL with protocol and port)
CSRF_TRUSTED_ORIGINS=http://yourdomain.com:8090,http://localhost:8090

# Timezone
TZ=UTC
